@{
    ViewBag.Title = "Home Page";
}
<table id="mainTable" class="table table-striped table-bordered" style="width:100%"></table>
<br />
<div class="form-check">
    <input class="form-check-input" type="checkbox" value="" id="SelectProblemDevices">
    <label class="form-check-label" for="SelectProblemDevices">
        Select Problem Devices
    </label>
</div>
<hr />

<br />

<ul class="nav nav-tabs" id="tankToadTabs" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="dataLogTable-tab" data-toggle="tab" href="#dataLogTab" role="tab" aria-controls="dataLogTab" aria-selected="true">Data log</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="diagnosticLogTable-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile" aria-selected="false">Diagnostic log</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="DeviceAttributesLogTable-tab" data-toggle="tab" href="#contact" role="tab" aria-controls="contact" aria-selected="false">Device Attributes log</a>
    </li>
    <li class="nav-item">
        <a class="nav-link" id="SMSlogTable-tab" data-toggle="tab" href="#SMSlogTab" role="tab" aria-controls="SMSlogTab" aria-selected="false">SMS log</a>
    </li>
</ul>
<div class="tab-content" id="subTables">
    <br />
    <div class="tab-pane fade show active" id="dataLogTab" role="tabpanel" aria-labelledby="dataLog-tab">
        <div id="dataLogTable-dateBox"></div>
        <br>
        <table id="dataLogTable" class="table table-striped table-bordered" style="width:100%"></table>
    </div>
    <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
        <div id="diagnosticLogTable-dateBox"></div>
        <br>
        <table id="diagnosticLogTable" class="table table-striped table-bordered" style="width:100%"></table>
    </div>
    <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">
        <div id="DeviceAttributesLogTable-dateBox"></div>
        <br>
        <table id="DeviceAttributesLogTable" class="table table-striped table-bordered" style="width:700px"></table>
    </div>
    <div class="tab-pane fade" id="SMSlogTab" role="tabpanel" aria-labelledby="SMSlogTab-tab">
        <div id="SMSlogTable-dateBox"></div>
        <br>
        <table id="SMSlogTable" class="table table-striped table-bordered" style="width:100%"></table>
    </div>
</div>


<br />
<div id="googleMap" class="googleMap" style="height:500px;"></div>
<link href="~/Content/css/main.css" rel="stylesheet" />

<script src="~/Scripts/main/googleMap.js"></script>
<script src="~/Scripts/main/event.js"></script>
<script src="~/Scripts/main/render.js"></script>
<script src="~/Scripts/main/main.js"></script>

<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBZGS5hNF8bI-9W6ZxwnC6cEl4De-Sx6j0&callback=_Map.InitMap&language=en">
</script>
<script src="~/Scripts/main/googleMap.js"></script>